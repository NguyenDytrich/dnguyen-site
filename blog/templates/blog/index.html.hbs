{{#*inline "page"}}
<div>
	<ul class="p-0">
	{{#each paginator.objects}}
	<li>
		<div>
			<div class="blog-heading">
				<h2><a class="title-link" href="/{{id}}/{{slug}}">{{title}}</a></h2>
				<div class="date">{{date_repr}}</div>
			</div>
			<div class="blog-preview">
				<div>
					{{{preview}}}
				</div>
			</div>
			<div class="blog-continue">
				<a class="highlight" href="/{{id}}/{{slug}}">Continue</a>
			</div>
		</div>
	</li>
	{{/each}}
	</ul>
</div>
<div class="text-center">
	{{#with paginator}}
	<a class="paginate"
		{{#if has_prev}}
		 href="/?page={{prev_index}}"
		{{else}}
		style="opacity:0; cursor: default;"
		{{/if}}
	>prev</a>
	{{index}} of {{total_pages}}
	<a class="paginate"
		{{#if has_next}}
		href="/?page={{next_index}}"
		{{else}}
		style="opacity:0; cursor: default;"
		{{/if}}
	>next</a>
	{{/with}}
</div>
{{/inline}}
{{~> (parent)~}}
